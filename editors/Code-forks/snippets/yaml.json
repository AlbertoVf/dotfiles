{
	"puppeteer - markdown preview": {
		"prefix": "print",
		"description": "puppeteer - markdown preview. Configuracion Yaml para exportar a pdf con markdown preview",
		"body": [
			"puppeteer:",
			"\tformat: A4",
			"\tlandscape: ${1|false,true|}",
			"\tdisplayHeaderFooter: ${2|true,false|}",
			"\tfooterTemplate: \"<div style='width:100%; text-transform:uppercase; text-align:center; font: normal bold normal 11px Courier;'>[ <span class='title'></span> - <span class='pageNumber'></span> ]</span></div>\"",
			"\theaderTemplate: None",
			"\tmargin:",
			"\t\tbottom: 2cm",
			"\t\ttop: 2cm",
			"\t\tright: 2cm",
			"\t\tleft: 2cm"
		]
	}
}
