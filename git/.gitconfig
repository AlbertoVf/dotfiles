[credential]
	helper = store
[color]
	ui = true
[color "status"]
	added     = green
	changed   = yellow
	untracked = red
[core]
	autocrlf     = input
	editor       = code --wait --profile GIT
	eol          = lf
	filemode     = false
	symlinks     = false
	whitespace   = trailing-space
	excludesfile = ~/.gitignore_global
[diff]
	colorMoved = default
	tool       = code
[difftool]
	prompt = false
[difftool "code"]
	cmd = code --profile GIT --wait --diff --reuse-window $LOCAL $REMOTE
[filter "lfs"]
	clean    = git-lfs clean -- %f
	process  = git-lfs filter-process
	required = true
	smudge   = git-lfs smudge -- %f
[gui]
	editor = code
[init]
	defaultBranch = main
[merge]
	conflictstyle = diff3
	tool          = code
[mergetool]
	keepBackup = false
[mergetool "code"]
	cmd = code --profile GIT --wait --reuse-window $MERGED
[push]
	default = current
[sequence]
	editor = code --profile GIT --wait --reuse-window
