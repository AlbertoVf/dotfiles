#!/usr/bin/env bash
# Generated on 2024-05-20 19:06

Print
	xfce4-screenshooter
XF86AudioLowerVolume
	amixer set Master 5%-
XF86AudioRaiseVolume
	amixer set Master 5%+
XF86AudioMute
	amixer -D pulse set Master 1+ toggle
XF86AudioNext
	playerctl next
XF86AudioPlay
	playerctl play-pause
XF86AudioPrev
	playerctl previous
XF86AudioStop
	playerctl stop
XF86MonBrightnessDown
	brightnessctl s 10%-
XF86MonBrightnessUp
	brightnessctl s +10%
alt + Escape
