{
    "[html]": { "editor.defaultFormatter": "vscode.html-language-features" },
    "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
    "[jsonc]": { "editor.defaultFormatter": "vscode.json-language-features" },
    "[markdown]": { "editor.defaultFormatter": "yzhang.markdown-all-in-one" },
    "[python]": { "editor.defaultFormatter": "ms-python.autopep8" },
    "[scss]": { "editor.defaultFormatter": "vscode.css-language-features" },
    "[typescript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
    "[typescriptreact]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
    "[yaml]": { "editor.defaultFormatter": "redhat.vscode-yaml" },
    "better-comments.tags": [
        { "tag": "todo", "color": "#3498DB" },
        { "tag": "!", "color": "#ff3636", "bold": true },
        { "tag": "?", "color": "#FF8C00", "italic": true },
        { "tag": "*", "color": "#00ff00", "italic": true },
        { "tag": "::", "color": "#ff41a6", "italic": true },
        { "tag": "//", "color": "#474747", "strikethrough": true },
    ],
    "betterAlign.operatorPadding": "right",
    "betterAlign.surroundSpace": {
        "arrow": [ 1, 1 ],
        "assignment": [ 1, 1 ],
        "colon": [ 1, 1 ],
        "comment": 2
    },
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "typescript": "deno run $dir$fileName",
        "csharp": "dotnet run $dir",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt"
    },
    "code-runner.saveFileBeforeRun": true,
    "codesnap.containerPadding": "2px",
    "codesnap.transparentBackground": true,
    "cSpell.language": "es-ES,en",
    "css.lint.duplicateProperties": "warning",
    "database-client.highlightSQLBlock": true,
    "database-client.showProcedure": false,
    "debug.console.closeOnEnd": true,
    "debug.terminal.clearBeforeReusing": true,
    "dev.containers.defaultExtensions": [
        "formulahendry.code-runner",
        "adpyke.codesnap",
        "naumovs.color-highlight",
        "redjue.git-commit-plugin",
        "christian-kohler.path-intellisense"
    ],
    "diffEditor.codeLens": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true
    },
    "editor.cursorBlinking": "expand",
    "editor.fontFamily": "CaskaydiaCove Nerd Font, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontVariations": true,
    "editor.guides.bracketPairs": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.letterSpacing": 1.5,
    "editor.lineHeight": 2.1,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.parameterHints.cycle": true,
    "editor.renderWhitespace": "all",
    "editor.scrollbar.scrollByPage": true,
    "editor.smoothScrolling": true,
    "editor.suggest.showDeprecated": false,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.tabCompletion": "on",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.wrappingStrategy": "advanced",
    "emmet.excludeLanguages": [],
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "emojisense.languages": {
        "git-commit": true,
        "makefile": true,
        "markdown": true,
        "plaintext": { "markupCompletionsEnabled": false, "emojiDecoratorsEnabled": false },
        "scminput": true,
    },
    "errorLens.enableOnDiffView": true,
    "errorLens.fontStyleItalic": true,
    "errorLens.fontWeight": "900",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        ".editorconfig": ".gitignore,Makefile,config*",
        ".env": "*.env, .env.*, .envrc, env.d.ts",
        ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*,.gitconfig",
        "*.css": "$(capture).css.map, $(capture).*.css",
        "*.html": "${capture}.css, ${capture}.js",
        "*.java": "$(capture).class",
        "*.js": "$(capture).js.map, $(capture).*.js, $(capture)_*.js",
        "*.md": "$(capture)*.pdf,$(capture)*.jpg,$(capture)*.png,$(capture)*.svg",
        "*.py": "${basename}_test.${extname},${basename}test.${extname},test${basename}.${extname},test_${basename}.${extname},",
        "*.ts": "$(capture).js, $(capture).d.ts.map, $(capture).*.ts, $(capture)_*.js, $(capture)_*.ts",
        "*.tsx": "$(capture).ts, $(capture).css",
        "Dockerfile": ".dockerignore, docker-compose.*, dockerfile*",
        "Makefile": ".env, Dockerfile, docker-compose.*,requirements.txt,.editorconfig, package.json, .*lint.yml,*.lock",
        "package.json": ".browserslist*, .circleci*, .commitlint*, .cz-config.js, .czrc, .dlint.json, .dprint.json, .editorconfig, .eslint*, .firebase*, .flowconfig, .github*, .gitlab*, .gitpod*, .huskyrc*, .jslint*, .lintstagedrc*, .markdownlint*, .node-version, .nodemon*, .npm*, .nvmrc, .pm2*, .pnp.*, .pnpm*, .prettier*, .release-please*.json, .releaserc*, .sentry*, .simple-git-hooks*, .stackblitz*, .styleci*, .stylelint*, .tazerc*, .textlint*, .tool-versions, .travis*, .versionrc*, .vscode*, .watchman*, .xo-config*, .yamllint*, .yarnrc*, Procfile, apollo.config.*, appveyor*, azure-pipelines*, bower.json, build.config.*, commitlint*, crowdin*, dangerfile*, dlint.json, dprint.json, electron-builder.*, eslint*, firebase.json, grunt*, gulp*, jenkins*, lerna*, lint-staged*, nest-cli.*, netlify*, nodemon*, npm-shrinkwrap.json, nx.*, package-lock.json, package.nls*.json, phpcs.xml, pm2.*, pnpm*, prettier*, pullapprove*, pyrightconfig.json, release-please*.json, release-tasks.sh, release.config.*, renovate*, rollup.config.*, rspack*, simple-git-hooks*, stylelint*, tslint*, tsup.config.*, turbo*, typedoc*, unlighthouse*, vercel*, vetur.config.*, webpack*, workspace.json, xo.config.*, yarn*",
        "README.md": "*.md,.editorconfig",
        "requirements.txt": ".editorconfig, .flake8, .isort.cfg, .python-version, requirements*.in, requirements*.pip, requirements*.txt, tox.ini",
    },
    "explorer.incrementalNaming": "smart",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 60000,
    "files.defaultLanguage": "markdown",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.autoRepositoryDetection": "openEditors",
    "git.autoStash": true,
    "git.closeDiffOnOperation": true,
    "git.detectSubmodules": true,
    "git.followTagsWhenSync": true,
    "git.mergeEditor": true,
    "githubIssues.queries": [
        { "label": "Closed Issues", "query": "author:${user} state:closed repo:${owner}/${repository} sort:created-asc" },
        { "label": "Created Issues", "query": "author:${user} state:open repo:${owner}/${repository} sort:created-asc" },
        { "label": "My Issues", "query": "default" }
    ],
    "githubPullRequests.defaultMergeMethod": "squash",
    "githubPullRequests.queries": [
        { "label": "Assigned To Me", "query": "is:open assignee:${user}" },
        { "label": "Created By Me", "query": "is:open author:${user}" },
        { "label": "Waiting For My Review", "query": "is:open review-requested:${user}" }
    ],
    "gitlens.graph.layout": "editor",
    "hediet.vscode-drawio.theme": "Kennedy",
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.templating": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "markdown-preview-github-styles.colorTheme": "light",
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "markdown.extension.toc.levels": "2..4",
    "markdown.extension.toc.orderedList": true,
    "markdown.marp.pdf.noteAnnotations": true,
    "markdown.marp.pdf.outlines": "headings",
    "markdown.math.enabled": true,
    "markdown.occurrencesHighlight.enabled": true,
    "markdown.preview.breaks": true,
    "markdown.updateLinksOnFileMove.enabled": "always",
    "markdownExtended.exportOutDirName": "PDFs",
    "markdownExtended.pdfDisplayHeaderFooter": true,
    "markdownExtended.pdfFooterTemplate": "<div style=\"font-size: 9px; font-family: monospace; margin: 0 auto;\"><span class='title'></span> - [<span class='pageNumber'></span>]</span></div>",
    "markdownExtended.pdfHeaderTemplate": "<div></div>",
    "markdownExtended.pdfMarginBottom": "2cm",
    "markdownExtended.pdfMarginLeft": "1.5cm",
    "markdownExtended.pdfMarginRight": "1.5cm",
    "markdownExtended.pdfMarginTop": "2cm",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "mergeEditor.diffAlgorithm": "advanced",
    "path-intellisense.showHiddenFiles": false,
    "plantuml.exportFormat": "png",
    "problems.defaultViewMode": "table",
    "projectManager.git.baseFolders": [ "~/Proyectos/" ],
    "projectManager.groupList": true,
    "projectManager.ignoreProjectsWithinProjects": true,
    "projectManager.supportSymlinksOnBaseFolders": true,
    "search.mode": "newEditor",
    "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "settingsSync.keybindingsPerPlatform": false,
    "symbols.hidesExplorerArrows": false,
    "tabnine.experimentalAutoImports": false,
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.smoothScrolling": true,
    "testExplorer.mergeSuites": true,
    "testExplorer.sort": "byLabelWithSuitesFirst",
    "testExplorer.useNativeTesting": true,
    "testing.alwaysRevealTestOnStateChange": true,
    "testing.automaticallyOpenPeekView": "failureAnywhere",
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.closeWhenEmpty": true,
    "window.menuBarVisibility": "toggle",
    "window.openFilesInNewWindow": "default",
    "window.restoreWindows": "none",
    "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Tokyo Midnight",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "symbols",
    "workbench.list.openMode": "doubleClick",
    "workbench.tree.indent": 16,
}