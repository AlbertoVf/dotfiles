{
    "[css]": { "editor.defaultFormatter": "rvest.vs-code-prettier-eslint" },
    "[dockerfile]": { "editor.defaultFormatter": "ms-azuretools.vscode-docker" },
    "[dotenv]": { "editor.defaultFormatter": "foxundermoon.shell-format" },
    "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[ignore]": { "editor.defaultFormatter": "foxundermoon.shell-format" },
    "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
    "[kotlin]": { "editor.defaultFormatter": "esafirm.kotlin-formatter" },
    "[markdown]": { "editor.defaultFormatter": "rvest.vs-code-prettier-eslint" },
    "[python]": { "editor.defaultFormatter": "ms-python.python" },
    "[scss]": { "editor.defaultFormatter": "vscode.css-language-features" },
    "[shellscript]": { "editor.defaultFormatter": "foxundermoon.shell-format" },
    "[sql]": { "editor.defaultFormatter": "cweijan.vscode-database-client2" },
    "[typescript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
    "[yaml]": { "editor.defaultFormatter": "redhat.vscode-yaml" },
    "betterAlign.operatorPadding": "right",
    "betterAlign.surroundSpace": { "colon": [1, 1], "assignment": [1, 1], "arrow": [1, 1], "comment": 2 },
    "bookmarks.useWorkaroundForFormatters": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": { "typescript": "deno run $dir$fileName" },
    "code-runner.saveFileBeforeRun": true,
    "codesnap.containerPadding": "3px",
    "codesnap.showWindowControls": true,
    "codesnap.transparentBackground": true,
    "css.format.spaceAroundSelectorSeparator": true,
    "css.lint.duplicateProperties": "warning",
    "csv-edit.doubleClickColumnHandleForcedWith": 50,
    "database-client.highlightSQLBlock": true,
    "database-client.showProcedure": false,
    "database-client.showView": false,
    "database-client.terminalLocaltion": "Panel",
    "debug.console.closeOnEnd": true,
    "debug.terminal.clearBeforeReusing": true,
    "dev.containers.defaultExtensions": [
        "adpyke.codesnap",
        "chouzz.vscode-better-align",
        "christian-kohler.path-intellisense",
        "codezombiech.gitignore",
        "cschleiden.vscode-github-actions",
        "donjayamanne.githistory",
        "EditorConfig.EditorConfig",
        "esbenp.prettier-vscode",
        "formulahendry.code-runner",
        "foxundermoon.shell-format",
        "GitHub.codespaces",
        "GitHub.remotehub",
        "GitHub.vscode-pull-request-github",
        "Gruntfuggly.todo-tree",
        "hbenl.vscode-test-explorer",
        "kisstkondoros.vscode-gutter-preview",
        "mhutchie.git-graph",
        "mintlify.document",
        "ms-vscode.vscode-github-issue-notebooks",
        "naumovs.color-highlight",
        "Perkovec.emoji",
        "rangav.vscode-thunder-client",
        "redjue.git-commit-plugin",
        "TabNine.tabnine-vscode",
        "tomoki1207.pdf",
        "VisualStudioExptTeam.intellicode-api-usage-examples",
        "VisualStudioExptTeam.vscodeintellicode",
        "wayou.vscode-todo-highlight"
    ],
    "diffEditor.codeLens": true,
    "docwriter.progress.trackClasses": true,
    "editor.autoIndent": "advanced",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.codeActionsOnSave": { "source.fixAll": true, "source.organizeImports": true },
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": true,
    "editor.fontFamily": "CaskaydiaCove Nerd Font,CaskaydiaCove NF, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "600",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToImplementation",
    "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.inlayHints.fontSize": 11,
    "editor.inlayHints.padding": true,
    "editor.inlineSuggest.enabled": true,
    "editor.letterSpacing": 0.5,
    "editor.lineHeight": 25,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fit",
    "editor.padding.bottom": 20,
    "editor.padding.top": 20,
    "editor.parameterHints.cycle": true,
    "editor.renderWhitespace": "all",
    "editor.scrollbar.scrollByPage": true,
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.stickyScroll.enabled": true,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showDeprecated": false,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "deepIndent",
    "emmet.excludeLanguages": [],
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "errorLens.enableOnDiffView": true,
    "errorLens.fontStyleItalic": true,
    "errorLens.fontWeight": "900",
    "explorer.fileNesting.enabled": true,
    "explorer.incrementalNaming": "smart",
    "extensions.experimental.useUtilityProcess": false,
    "files.autoGuessEncoding": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "files.eol": "\n",
    "files.exclude": {
        "*.git": true,
        "*.meta": true,
        "**.meta": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.idea": true,
        "**/.project": true,
        "**/.settings": true,
        "**/*.meta": true
    },
    "files.insertFinalNewline": true,
    "files.maxMemoryForLargeFilesMB": 2048,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.autoRepositoryDetection": "openEditors",
    "git.autoStash": true,
    "git.closeDiffOnOperation": true,
    "git.detectSubmodules": true,
    "git.followTagsWhenSync": true,
    "git.mergeEditor": true,
    "GitCommitPlugin.MaxSubjectCharacters": 30,
    "github-actions.workflows.pinned.refresh.enabled": true,
    "githubIssues.queries": [
        {
            "label": "Closed Issues",
            "query": "author:${user} state:closed repo:${owner}/${repository} sort:created-asc"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-asc"
        },
        { "label": "My Issues", "query": "default" }
    ],
    "githubPullRequests.defaultMergeMethod": "squash",
    "githubPullRequests.queries": [
        { "label": "Assigned To Me", "query": "is:open assignee:${user}" },
        { "label": "Created By Me", "query": "is:open author:${user}" },
        { "label": "Waiting For My Review", "query": "is:open review-requested:${user}" }
    ],
    "hediet.vscode-drawio.theme": "Kennedy",
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.templating": true,
    "html.format.wrapLineLength": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "less.format.spaceAroundSelectorSeparator": true,
    "livePreview.openPreviewTarget": "External Browser",
    "livePreview.showServerStatusNotifications": true,
    "markdown-mermaid.darkModeTheme": "neutral",
    "markdown-pdf.breaks": true,
    "markdown-pdf.displayHeaderFooter": false,
    "markdown-pdf.highlightStyle": "github.css",
    "markdown-pdf.includeDefaultStyles": true,
    "markdown-pdf.margin.bottom": "1.5cm",
    "markdown-pdf.omitBackground": true,
    "markdown-pdf.styles": ["~/.config/Code/User/markdown-pdf.css"],
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "markdown.extension.toc.levels": "1..3",
    "markdown.extension.toc.orderedList": true,
    "markdown.extension.toc.unorderedList.marker": "-",
    "markdown.marp.pdf.noteAnnotations": true,
    "markdown.marp.pdf.outlines": "headings",
    "markdown.occurrencesHighlight.enabled": true,
    "markdown.preview.breaks": true,
    "markdown.preview.typographer": true,
    "markdown.updateLinksOnFileMove.enabled": "prompt",
    "markdownExtended.pdfDisplayHeaderFooter": false,
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "mergeEditor.diffAlgorithm": "smart",
    "notebook.cellToolbarVisibility": "hover",
    "notebook.diff.ignoreMetadata": true,
    "notebook.lineNumbers": "on",
    "notebook.markup.fontSize": 15,
    "notebook.outline.showCodeCells": true,
    "notebook.output.textLineLimit": 15,
    "notebook.outputFontSize": 10,
    "npm.enableRunFromFolder": true,
    "prettier.bracketSameLine": true,
    "prettier.printWidth": 120,
    "prettier.proseWrap": "never",
    "prettier.tabWidth": 4,
    "problems.defaultViewMode": "table",
    "problems.showCurrentInStatus": true,
    "python.analysis.autoImportCompletions": true,
    "python.linting.": true,
    "python.linting.enabled": true,
    "references.preferredLocation": "view",
    "scm.alwaysShowActions": true,
    "scm.diffDecorationsGutterPattern": { "added": true },
    "scm.diffDecorationsIgnoreTrimWhitespace": "true",
    "scm.inputFontFamily": "editor",
    "scm.repositories.sortOrder": "path",
    "scss.format.preserveNewLines": false,
    "scss.format.spaceAroundSelectorSeparator": true,
    "search.mode": "newEditor",
    "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
    "search.seedOnFocus": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "settingsSync.keybindingsPerPlatform": false,
    "tabnine.experimentalAutoImports": false,
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.smoothScrolling": true,
    "testExplorer.hideWhen": "noTests",
    "testExplorer.mergeSuites": true,
    "testExplorer.onReload": "reset",
    "testExplorer.sort": "byLocation",
    "testExplorer.useNativeTesting": true,
    "testing.alwaysRevealTestOnStateChange": true,
    "testing.automaticallyOpenPeekView": "failureAnywhere",
    "testing.automaticallyOpenPeekViewDuringAutoRun": true,
    "thunder-client.indentSize": 4,
    "todo-tree.general.tags": ["FIXME", "NOTE", "REVIEW", "TODO", "TEST"],
    "todo-tree.highlights.customHighlight": {
        "FIXME": { "icon": "alert-fill", "iconColour": "#ff41a6" },
        "NOTE": { "icon": "pencil", "iconColour": "#0da235" },
        "REVIEW": { "icon": "shield", "iconColour": "#ff3636" },
        "TEST": { "icon": "zap", "iconColour": "#ffdd00" },
        "TODO": { "icon": "check-circle", "iconColour": "#3eaeff" }
    },
    "todo-tree.highlights.defaultHighlight": { "icon": "alert", "type": "text", "background": "#00000000" },
    "todohighlight.defaultStyle": {
        "backgroundColor": "#fafafa00",
        "fontStyle": "italic",
        "fontWeight": "bold",
        "isWholeLine": true
    },
    "todohighlight.keywords": [
        { "text": "FIXME:", "color": "#ff41a6", "backgroundColor": "#fafafa00" },
        { "text": "NOTE:", "color": "#0da235" },
        { "text": "REVIEW:", "color": "#ff3636" },
        { "text": "TEST:", "color": "#ffdd00" },
        { "text": "TODO:", "color": "#3eaeff", "backgroundColor": "#fafafa00" }
    ],
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.closeWhenEmpty": true,
    "window.openFilesInNewWindow": "default",
    "window.restoreWindows": "none",
    "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
    "window.titleSeparator": " /",
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Andromeda Italic Bordered",
    "workbench.commandPalette.history": 15,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 5,
    "workbench.editor.splitSizing": "split",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.multiSelectModifier": "ctrlCmd",
    "workbench.list.openMode": "doubleClick",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.tree.indent": 16,
    "workbench.view.alwaysShowHeaderActions": true,
    "yaml.format.proseWrap": "always",
    "yaml.format.singleQuote": true
}
