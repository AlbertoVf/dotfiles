{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[scss]": {
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ]
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "C_Cpp.intelliSenseEngine": "Disabled",
  "css.lint.duplicateProperties": "warning",
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.codeLens": true,
  "editor.fontFamily": "FantasqueSansMono Nerd Font, Anoymice Nerd Font, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "deepIndent",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.incrementalNaming": "smart",
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 10000,
  "files.eol": "\n",
  "files.exclude": {
    "*.meta": true,
    "**.meta": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.meta": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.inputValidationLength": 120,
  "gitlens.views.remotes.branches.layout": "list",
  "hediet.vscode-drawio.theme": "Kennedy",
  "html.format.contentUnformatted": "",
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "less.lint.duplicateProperties": "warning",
  "markdown.preview.typographer": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.executeInFileDir": true,
  "scss.lint.duplicateProperties": "warning",
  "search.showLineNumbers": true,
  "sqltools.connections": [],
  "sqltools.useNodeRuntime": true,
  "tabnine.experimentalAutoImports": true,
  "timeline.excludeSources": [],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": true,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
  "window.titleSeparator": " /",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.list.openMode": "doubleClick",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Dracula",
  "workbench.preferredLightColorTheme": "Quiet Light",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.colorTheme": "Palenight Italic",
  "cmake.configureOnOpen": true,
  "editor.inlineSuggest.enabled": true,
  "arduino.enableUSBDetection": true,
  "arduino.openPDEFiletype": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false
  },
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "editor.tabSize": 2,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}