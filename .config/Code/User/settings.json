{
  "[css]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[html]": {
    "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[scss]": {
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ],
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "bashBeautify.tabSize": 2,
  "C_Cpp.intelliSenseEngine": "Disabled",
  "css.lint.duplicateProperties": "warning",
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.codeLens": true,
  "editor.fontFamily": "Anonymice NF,FantasqueSansMono Nerd Font, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "deepIndent",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.incrementalNaming": "smart",
  "files.associations": {
    "*.html": "html"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 10000,
  "files.eol": "\n",
  "files.exclude": {
    "*.meta": true,
    "**.meta": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.meta": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.inputValidationLength": 120,
  "gitlens.views.remotes.branches.layout": "list",
  "hediet.vscode-drawio.local-storage": "",
  "hediet.vscode-drawio.theme": "Kennedy",
  "html.format.contentUnformatted": "",
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "kite.showWelcomeNotificationOnStartup": false,
  "less.lint.duplicateProperties": "warning",
  "markdown.preview.typographer": true,
  "python.showStartPage": false,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.executeInFileDir": true,
  "redhat.telemetry.enabled": true,
  "scss.lint.duplicateProperties": "warning",
  "search.showLineNumbers": true,
  "sqltools.connections": [],
  "sqltools.useNodeRuntime": true,
  "tabnine.experimentalAutoImports": true,
  "timeline.excludeSources": [],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": true,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
  "window.titleSeparator": " /",
  "workbench.colorTheme": "Dracula",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.iconTheme": "helium-icon-theme",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.list.openMode": "doubleClick",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Dracula",
  "workbench.preferredLightColorTheme": "GitHub Plus",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.tree.indent": 16,
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  "[shellscript]": {
    "editor.defaultFormatter": "shakram02.bash-beautify"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}
