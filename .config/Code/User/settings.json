{
  "[java]": { "editor.defaultFormatter": "redhat.java" },
  "[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
  "[python]": { "editor.defaultFormatter": "ms-python.python" },
  "[typescript]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "diffEditor.codeLens": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.fontFamily": "CaskaydiaCove Nerd Font,FantasqueSansMono Nerd Font, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "deepIndent",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.incrementalNaming": "smart",
  "files.associations": { "*.html": "html" },
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 10000,
  "files.eol": "\n",
  "files.exclude": { "*.meta": true, "**.meta": true, "**/.classpath": true, "**/.factorypath": true, "**/.project": true, "**/.settings": true, "**/*.meta": true },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.inputValidationLength": 120,
  "gitlens.views.remotes.branches.layout": "list",
  "hediet.vscode-drawio.local-storage": "",
  "hediet.vscode-drawio.theme": "Kennedy",
  "html.format.contentUnformatted": "",
  "html.format.wrapLineLength": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.showStartPage": false,
  "tabnine.experimentalAutoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.restoreWindows": "none",
  "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
  "window.titleSeparator": " /",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.editorAssociations": { "*.ipynb": "jupyter.notebook.ipynb" },
  "workbench.iconTheme": "helium-icon-theme",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.list.openMode": "doubleClick",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.tree.indent": 16,
  "C_Cpp.intelliSenseEngine": "Disabled",
  "[c]": { "editor.defaultFormatter": "ms-vscode.cpptools" },
  "kite.showWelcomeNotificationOnStartup": false,
  "sqltools.connections": [],
  "[shellscript]": { "editor.defaultFormatter": "shakram02.bash-beautify" },
  "workbench.colorTheme": "Dracula",
  "window.closeWhenEmpty": true,
  "search.showLineNumbers": true,
  "debug.terminal.clearBeforeReusing": true,
  "[scss]": { "editor.suggest.insertMode": "replace", "gitlens.codeLens.scopes": ["document"], },
  "redhat.telemetry.enabled": true,
  "timeline.excludeSources": [],
  "sqltools.useNodeRuntime": true
}
