<action>
	<icon>link</icon>
	<name>Crear link</name>
	<unique-id>1662556204328140-2</unique-id>
	<command>  OUTPUT=$(zenity --forms --title=&quot;Crea un link&quot; --text=&quot;Crea un link a una pagina&quot; --separator=&quot;,&quot; --add-entry=&quot;url de la pagina&quot; --add-entry=&quot;nombre del fichero&quot;)
  accepted=$?
  if ((accepted != 0)); then
      exit 1
  fi
  url=$(awk -F, &apos;{print $1}&apos; &lt;&lt;&lt;$OUTPUT)
  nombre=$(awk -F, &apos;{print $2}&apos; &lt;&lt;&lt;$OUTPUT)
  echo -e &quot;[Desktop Entry]\nVersion=1.0\nType=Link\nName=$nombre\nComment=\nIcon=user-bookmarks\nURL=$url&quot; &gt; &quot;$nombre&quot;.desktop</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
